mutation{
          update_user_db_v2_warehouse(
            where: {product_id: {_eq: $product_id} , version : {_eq: $version }}
            _inc: {active_number: $active_number_change,  reverse_number: $reverse_number_change, version: 1}
          ) { returning{
            product_id
            active_number
            reverse_number
            version
          }
          }
        }