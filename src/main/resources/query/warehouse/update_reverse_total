mutation{
          update_user_db_v2_warehouse(
            where: {product_id: {_eq: $product_id}, version : {_eq: $version }},
            _inc: {total: -$total,  reverse_number: -$total, version: 1}
          ) { returning{
            product_id
            total
            active_number
            reverse_number
            version
          }
          }
        }